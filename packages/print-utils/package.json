{
  "name": "print-utils",
  "description": "Print utilities for client web applications.",
  "version": "0.15.0",
  "main": "lib/index.js",
  "jsnext:main": "dist/bundle.es.js",
  "files": [
    "lib",
    "dist"
  ],
  "jest": {
    "testPathIgnorePatterns": [
      "/__fixtures__/"
    ]
  },
  "scripts": {
    "start": "npm run build -- --watch",
    "prebuild": "rimraf lib dist",
    "build:dist": "rollup -c --sourcemap inline --environment NODE_ENV:production",
    "build": "babel src -d lib --ignore __tests__,__mocks__",
    "preversion": "npm test && npm run build && npm run build:dist",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-plugin-transform-runtime": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-latest": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-2": "latest",
    "babel-register": "latest",
    "babelrc-rollup": "^3.0.0",
    "bin-utils": "latest",
    "eslint": "latest",
    "eslint-plugin-babel": "latest",
    "jest": "latest",
    "ncp": "latest",
    "npm-run-all": "latest",
    "rimraf": "latest",
    "rollup": "latest",
    "rollup-plugin-babel": "latest",
    "rollup-plugin-commonjs": "latest",
    "rollup-plugin-node-resolve": "latest",
    "rollup-watch": "latest",
    "sinon": "^1.17.6"
  },
  "dependencies": {
    "browser-detective": "^0.15.0",
    "classnames": "^2.2.5",
    "invariant": "^2.2.1",
    "raf": "^3.3.0"
  }
}
